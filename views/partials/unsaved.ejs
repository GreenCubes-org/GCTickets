<div class="ui modal" id="unsavedlink">
	<div class="header">
	<%- __('view.create.partials.unsaved.title') %>
	</div>
	<div class="content">
	  <div class="left">
		<i class="remove sign icon"></i>
	  </div>
	  <div class="right">
		<p><%- __('view.create.partials.unsaved.description') %></p>
	  </div>
	</div>
	<div class="actions">
	  <div class="two fluid ui buttons">
		<div class="ui negative labeled icon button">
			<i class="remove icon"></i>
			<%- __('view.create.partials.unsaved.action.no') %>
		</div>
		<div class="ui positive right labeled icon button" type="submit">
			<%- __('view.create.partials.unsaved.action.yes') %>
			<i class="checkmark icon"></i>
		</div>
	  </div>
	</div>
</div>
