<div class="ui modal" id="gc-logoutmodal">
	<div class="header">
	<%- __('view.partials.logoutmodal.header') %>
	</div>
	<div class="content">
	  <div class="left">
		<i class="sign out icon"></i>
	  </div>
	  <div class="right">
		<p><%- __('view.partials.logoutmodal.description') %></p>
	  </div>
	</div>
	<div class="actions">
	  <div class="two fluid ui buttons">
		<div class="ui negative labeled icon button">
			<i class="remove icon"></i>
			<%- __('view.partials.logoutmodal.action.no') %>
		</div>
		<div class="ui positive right labeled icon button" type="submit">
			<%- __('view.partials.logoutmodal.action.yes') %>
			<i class="checkmark icon"></i>
		</div>
	  </div>
	</div>
</div>
