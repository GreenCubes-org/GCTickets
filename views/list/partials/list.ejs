<div class="ui selection list">
	<% tickets.forEach(function(ticket) { %>
	<div class="item" style="color:inherit !important;padding:0 !important;border-top: 1px solid rgba(0, 0, 0, 0.1) !important;height: 80px !important;">
		<div class="right floated t-status <%= ticket.status.class %>"<% if (ticket.comments.lastChangedToOwner) { %> title="Статус установлен <%= ticket.comments.lastChangedToOwner %>"<% } %> style="padding:2.2em 1em;"><%- ticket.status.text %></div>
		<i class="right floated big <%= ticket.type.iconclass %> icon"  style="margin: 22px 10px 0px !important" title="<%= ticket.type.descr %>"></i>

		<a class="content gc-nostylelink" style="display:block !important;padding: 17px 0px !important;font-size:1.2em !important;" href="/id/<%= ticket.id %>">
			<div class="header" style="margin-left: 20px !important;"><span style="font-weight: 400 !important;">#<%= ticket.id %>:</span> <%= ticket.title %></div>
			<div class="description" style="font-size: 0.9em !important;margin-left: 20px !important;">
				<i class="<%= ticket.visibility.class %> small icon" style="margin:0 !important;" title="<%= ticket.visibility.text %>"></i>
				<i class="user small icon"></i><%= ticket.owner.login %>
				<span title="<%= moment(ticket.createdAt).format('Do MMM YYYY, h:mm') %>"><i class="time small icon"></i><%= moment(ticket.createdAt).fromNow() %></span>
				<% if (ticket.comments.count) { %><span title="Последний ответивший: <%= ticket.comments.lastCommentOwner %>"><i class="comment small icon"></i><%= ticket.comments.count %></span><% } %>
				<% if (ticket.product && ticket.product.ticketText.length) { %><i class="map marker icon"></i><%= ticket.product.ticketText %><% } %>
			</div>
		</a>
	</div>
	<% }) %>
</div>
