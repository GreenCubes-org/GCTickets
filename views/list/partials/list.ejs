<div class="ui selection list">
	<% tickets.forEach(function(ticket) { %>
	<div class="item" style="color:inherit !important;padding:0 !important;border-top: 1px solid rgba(0, 0, 0, 0.1) !important;height: 5rem !important;">
		<div class="right floated t-status <%= ticket.status.class %>"<% if (ticket.comments.lastChangedToOwner) { %> title="Статус установлен <%= ticket.comments.lastChangedToOwner %>"<% } %> style="padding:1.9rem 1rem;"><%- ticket.status.text %></div>
		<i class="right floated big <%= ticket.type.iconclass %> icon"  style="margin: 22px 10px 0px !important" title="<%= ticket.type.descr %>"></i>

		<a class="content gc-nostylelink" style="display:block !important;padding: 0.9em 0em !important;font-size:1.2rem !important;" href="/id/<%= ticket.id %>">
			<div class="l header" style="margin-left: 1rem !important;"><span style="font-weight: 400 !important;">#<%= ticket.id %>:</span> <span title="<%= ticket.title %>"><%= ticket.title %></span></div>
			<div class="l description" style="font-size: 1.1rem !important;margin-left: 1rem !important;">
				<i class="<%= ticket.visibility.class %> small icon" style="margin:0 !important;" title="<%= ticket.visibility.text %>"></i>
				<i class="user small icon"></i><%= ticket.owner.login %>
				<span title="<%= moment(ticket.createdAt).format('Do MMM YYYY, H:mm') %>"><i class="time small icon"></i><%= moment(ticket.createdAt).fromNow() %></span>
				<% if (ticket.comments.count) { %><span title="Последний ответивший: <%= ticket.comments.lastCommentOwner %>"><i class="comment small icon"></i><%= ticket.comments.count %> <small><%= ticket.comments.lastCommentOwner %></small></span><% } %>
				<% if (ticket.product && ticket.product.ticketText.length) { %><i class="map marker icon"></i><%= ticket.product.ticketText %><% } %>
			</div>
		</a>
	</div>
	<% }) %>
</div>
