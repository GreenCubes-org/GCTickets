<% title = 'GreenCubes.Тикеты'
styles.push('login.css');
scripts.push('login.page.js') %>
<div class="gc-center">
  <img class="ui image gc-logo gc-center" src="/images/logo-inverted.png">
  <h1>Тикет-система GreenCubes<br>
  <span class="hidden-xs">Откройся!</span></h1>
  <form id="loginform">
  <div class="ui form" style="display: inline-block !important">
    <div class="field">
      <div class="ui left labeled icon input">
        <input placeholder="Имя пользователя" type="text" name="username">
        <i class="user icon"></i>
      </div>
    </div>
    <div class="field">
      <div class="ui left labeled icon input">
        <input placeholder="Пароль" type="password" name="password">
        <i class="lock icon"></i>
      </div>
    </div>
    <button type="submit" class="ui blue submit button">Войти</button>
  </div>
  </form>
  <div id="loginerr"></div>
</div>
